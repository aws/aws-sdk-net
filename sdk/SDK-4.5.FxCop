      <RuleFile Name="$(ProjectDir)\..\buildtools\CustomFxCopRules\bin\Release\CustomFxCopRules.dll" Enabled="True" AllRulesEnabled="True" />
      <RuleFile Name="$(FxCopDir)\Rules\DesignRules.dll" Enabled="True" AllRulesEnabled="False">
