<?xml version="1.0" encoding="utf-16"?>
<property-value-rules>
  <property-value-rule>
    <property>Amazon.ElasticLoadBalancing.Model.DescribeLoadBalancersRequest.PageSize</property>
    <min>1</min>
    <max>400</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.ElasticLoadBalancing.Model.BackendServerDescription.InstancePort</property>
    <min>1</min>
    <max>65535</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.ElasticLoadBalancing.Model.ConnectionSettings.IdleTimeout</property>
    <min>1</min>
    <max>3600</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.ElasticLoadBalancing.Model.HealthCheck.HealthyThreshold</property>
    <min>2</min>
    <max>10</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.ElasticLoadBalancing.Model.HealthCheck.Interval</property>
    <min>1</min>
    <max>300</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.ElasticLoadBalancing.Model.HealthCheck.Timeout</property>
    <min>1</min>
    <max>300</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.ElasticLoadBalancing.Model.HealthCheck.UnhealthyThreshold</property>
    <min>2</min>
    <max>10</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.ElasticLoadBalancing.Model.Listener.InstancePort</property>
    <min>1</min>
    <max>65535</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.ElasticLoadBalancing.Model.Tag.Key</property>
    <min>1</min>
    <max>128</max>
    <pattern>^([\p{L}\p{Z}\p{N}_.:/=+\-@]*)$</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.ElasticLoadBalancing.Model.Tag.Value</property>
    <min>0</min>
    <max>256</max>
    <pattern>^([\p{L}\p{Z}\p{N}_.:/=+\-@]*)$</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.ElasticLoadBalancing.Model.TagKeyOnly.Key</property>
    <min>1</min>
    <max>128</max>
    <pattern>^([\p{L}\p{Z}\p{N}_.:/=+\-@]*)$</pattern>
  </property-value-rule>
</property-value-rules>