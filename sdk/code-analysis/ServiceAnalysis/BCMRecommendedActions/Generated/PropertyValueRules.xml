<?xml version="1.0" encoding="utf-16"?>
<property-value-rules>
  <property-value-rule>
    <property>Amazon.BCMRecommendedActions.Model.ListRecommendedActionsRequest.MaxResults</property>
    <min>1</min>
    <max>1000</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.BCMRecommendedActions.Model.ListRecommendedActionsRequest.NextToken</property>
    <min>1</min>
    <max>8192</max>
    <pattern>[\S\s]*</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.BCMRecommendedActions.Model.ListRecommendedActionsResponse.NextToken</property>
    <min>1</min>
    <max>8192</max>
    <pattern>[\S\s]*</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.BCMRecommendedActions.Model.RecommendedAction.AccountId</property>
    <min>12</min>
    <max>12</max>
    <pattern>[0-9]{12}</pattern>
  </property-value-rule>
</property-value-rules>