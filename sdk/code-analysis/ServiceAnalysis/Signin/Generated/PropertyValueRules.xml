<?xml version="1.0" encoding="utf-16"?>
<property-value-rules>
  <property-value-rule>
    <property>Amazon.Signin.Model.CreateOAuth2TokenRequestBody.ClientId</property>
    <pattern>arn:aws:signin:::devtools/(cross-device|same-device)</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.Signin.Model.CreateOAuth2TokenRequestBody.Code</property>
    <min>1</min>
    <max>512</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.Signin.Model.CreateOAuth2TokenRequestBody.CodeVerifier</property>
    <min>43</min>
    <max>128</max>
    <pattern>[A-Za-z0-9\-._~]+</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.Signin.Model.CreateOAuth2TokenRequestBody.GrantType</property>
    <pattern>(authorization_code|refresh_token)</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.Signin.Model.CreateOAuth2TokenRequestBody.RedirectUri</property>
    <min>1</min>
    <max>2048</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.Signin.Model.CreateOAuth2TokenRequestBody.RefreshToken</property>
    <min>1</min>
    <max>2048</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.Signin.Model.CreateOAuth2TokenResponseBody.ExpiresIn</property>
    <min>1</min>
    <max>900</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.Signin.Model.CreateOAuth2TokenResponseBody.IdToken</property>
    <min>1</min>
    <max>4096</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.Signin.Model.CreateOAuth2TokenResponseBody.RefreshToken</property>
    <min>1</min>
    <max>2048</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.Signin.Model.CreateOAuth2TokenResponseBody.TokenType</property>
    <pattern>aws_sigv4</pattern>
  </property-value-rule>
</property-value-rules>