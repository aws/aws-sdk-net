<?xml version="1.0" encoding="utf-16"?>
<property-value-rules>
  <property-value-rule>
    <property>Amazon.KinesisVideoMedia.Model.GetMediaRequest.StreamARN</property>
    <min>1</min>
    <max>1024</max>
    <pattern>arn:aws:kinesisvideo:[a-z0-9-]+:[0-9]+:[a-z]+/[a-zA-Z0-9_.-]+/[0-9]+</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.KinesisVideoMedia.Model.GetMediaRequest.StreamName</property>
    <min>1</min>
    <max>256</max>
    <pattern>[a-zA-Z0-9_.-]+</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.KinesisVideoMedia.Model.GetMediaResponse.ContentType</property>
    <min>1</min>
    <max>128</max>
    <pattern>^[a-zA-Z0-9_\.\-]+$</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.KinesisVideoMedia.Model.StartSelector.AfterFragmentNumber</property>
    <min>1</min>
    <max>128</max>
    <pattern>^[0-9]+$</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.KinesisVideoMedia.Model.StartSelector.ContinuationToken</property>
    <min>1</min>
    <max>128</max>
    <pattern>^[a-zA-Z0-9_\.\-]+$</pattern>
  </property-value-rule>
</property-value-rules>