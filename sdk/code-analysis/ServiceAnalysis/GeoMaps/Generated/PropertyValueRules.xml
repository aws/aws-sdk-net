<?xml version="1.0" encoding="utf-16"?>
<property-value-rules>
  <property-value-rule>
    <property>Amazon.GeoMaps.Model.GetGlyphsRequest.FontStack</property>
    <min>0</min>
    <max>1000</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.GeoMaps.Model.GetGlyphsRequest.FontUnicodeRange</property>
    <pattern>[0-9]+-[0-9]+\.pbf</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.GeoMaps.Model.GetSpritesRequest.FileName</property>
    <pattern>sprites(@2x)?\.(png|json)</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.GeoMaps.Model.GetStaticMapRequest.BoundedPositions</property>
    <min>7</min>
    <pattern>(-?\d{1,3}(\.\d{1,14})?,-?\d{1,2}(\.\d{1,14})?)(,(-?\d{1,3}(\.\d{1,14})?,-?\d{1,2}(\.\d{1,14})?))*</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.GeoMaps.Model.GetStaticMapRequest.BoundingBox</property>
    <min>7</min>
    <pattern>(-?\d{1,3}(\.\d{1,14})?,-?\d{1,2}(\.\d{1,14})?)(,(-?\d{1,3}(\.\d{1,14})?,-?\d{1,2}(\.\d{1,14})?))*</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.GeoMaps.Model.GetStaticMapRequest.Center</property>
    <min>3</min>
    <max>36</max>
    <pattern>-?\d{1,3}(\.\d{1,14})?,-?\d{1,2}(\.\d{1,14})?</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.GeoMaps.Model.GetStaticMapRequest.CompactOverlay</property>
    <min>1</min>
    <max>7000</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.GeoMaps.Model.GetStaticMapRequest.FileName</property>
    <pattern>map(@2x)?</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.GeoMaps.Model.GetStaticMapRequest.GeoJsonOverlay</property>
    <min>1</min>
    <max>7000</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.GeoMaps.Model.GetStaticMapRequest.Height</property>
    <min>64</min>
    <max>1400</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.GeoMaps.Model.GetStaticMapRequest.Key</property>
    <min>0</min>
    <max>1000</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.GeoMaps.Model.GetStaticMapRequest.Radius</property>
    <min>0</min>
    <max>4294967295</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.GeoMaps.Model.GetStaticMapRequest.Width</property>
    <min>64</min>
    <max>1400</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.GeoMaps.Model.GetStaticMapRequest.Zoom</property>
    <min>0</min>
    <max>20</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.GeoMaps.Model.GetStyleDescriptorRequest.Key</property>
    <min>0</min>
    <max>1000</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.GeoMaps.Model.GetStyleDescriptorRequest.PoliticalView</property>
    <min>2</min>
    <max>3</max>
    <pattern>([A-Z]{2}|[A-Z]{3})</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.GeoMaps.Model.GetTileRequest.Key</property>
    <min>0</min>
    <max>1000</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.GeoMaps.Model.GetTileRequest.Tileset</property>
    <min>1</min>
    <max>100</max>
    <pattern>[-.\w]+</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.GeoMaps.Model.GetTileRequest.X</property>
    <pattern>.*\d+.*</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.GeoMaps.Model.GetTileRequest.Y</property>
    <pattern>.*\d+.*</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.GeoMaps.Model.GetTileRequest.Z</property>
    <pattern>.*\d+.*</pattern>
  </property-value-rule>
</property-value-rules>