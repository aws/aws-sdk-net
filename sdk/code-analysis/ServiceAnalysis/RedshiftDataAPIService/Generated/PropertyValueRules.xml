<?xml version="1.0" encoding="utf-16"?>
<property-value-rules>
  <property-value-rule>
    <property>Amazon.RedshiftDataAPIService.Model.DescribeTableRequest.MaxResults</property>
    <min>0</min>
    <max>1000</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.RedshiftDataAPIService.Model.ExecuteStatementRequest.StatementName</property>
    <min>0</min>
    <max>500</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.RedshiftDataAPIService.Model.ListDatabasesRequest.MaxResults</property>
    <min>0</min>
    <max>1000</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.RedshiftDataAPIService.Model.ListSchemasRequest.MaxResults</property>
    <min>0</min>
    <max>1000</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.RedshiftDataAPIService.Model.ListStatementsRequest.MaxResults</property>
    <min>0</min>
    <max>100</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.RedshiftDataAPIService.Model.ListStatementsRequest.StatementName</property>
    <min>0</min>
    <max>500</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.RedshiftDataAPIService.Model.ListTablesRequest.MaxResults</property>
    <min>0</min>
    <max>1000</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.RedshiftDataAPIService.Model.SqlParameter.Name</property>
    <pattern>^[0-9a-zA-Z_]+$</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.RedshiftDataAPIService.Model.SqlParameter.Value</property>
    <min>1</min>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.RedshiftDataAPIService.Model.StatementData.StatementName</property>
    <min>0</min>
    <max>500</max>
  </property-value-rule>
</property-value-rules>