<?xml version="1.0" encoding="utf-16"?>
<property-value-rules>
  <property-value-rule>
    <property>Amazon.DirectConnect.Model.Tag.Key</property>
    <min>1</min>
    <max>128</max>
    <pattern>^([\p{L}\p{Z}\p{N}_.:/=+\-@]*)$</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.DirectConnect.Model.Tag.Value</property>
    <min>0</min>
    <max>256</max>
    <pattern>^([\p{L}\p{Z}\p{N}_.:/=+\-@]*)$</pattern>
  </property-value-rule>
</property-value-rules>