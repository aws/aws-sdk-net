<?xml version="1.0" encoding="utf-16"?>
<property-value-rules>
  <property-value-rule>
    <property>Amazon.DirectConnect.Model.ConfirmCustomerAgreementRequest.AgreementName</property>
    <max>100</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.DirectConnect.Model.ConfirmCustomerAgreementResponse.Status</property>
    <max>30</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.DirectConnect.Model.AssociatedCoreNetwork.AttachmentId</property>
    <min>12</min>
    <max>28</max>
    <pattern>^attachment-([0-9a-f]{1,17})$</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.DirectConnect.Model.AssociatedCoreNetwork.Id</property>
    <min>14</min>
    <max>30</max>
    <pattern>^core-network-([0-9a-f]{1,17})$</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.DirectConnect.Model.CustomerAgreement.AgreementName</property>
    <max>100</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.DirectConnect.Model.CustomerAgreement.Status</property>
    <max>30</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.DirectConnect.Model.Tag.Key</property>
    <min>1</min>
    <max>128</max>
    <pattern>^([\p{L}\p{Z}\p{N}_.:/=+\-@]*)$</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.DirectConnect.Model.Tag.Value</property>
    <min>0</min>
    <max>256</max>
    <pattern>^([\p{L}\p{Z}\p{N}_.:/=+\-@]*)$</pattern>
  </property-value-rule>
</property-value-rules>