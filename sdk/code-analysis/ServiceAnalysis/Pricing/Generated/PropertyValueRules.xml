<?xml version="1.0" encoding="utf-16"?>
<property-value-rules>
  <property-value-rule>
    <property>Amazon.Pricing.Model.DescribeServicesRequest.MaxResults</property>
    <min>1</min>
    <max>100</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.Pricing.Model.GetAttributeValuesRequest.MaxResults</property>
    <min>1</min>
    <max>100</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.Pricing.Model.GetPriceListFileUrlRequest.FileFormat</property>
    <min>1</min>
    <max>255</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.Pricing.Model.GetPriceListFileUrlRequest.PriceListArn</property>
    <min>18</min>
    <max>2048</max>
    <pattern>^arn:.+:pricing::.*:price-list/.{1,255}/.{1,32}/[A-Z]{3}/[0-9]{14}/[^/]*$</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.Pricing.Model.GetProductsRequest.MaxResults</property>
    <min>1</min>
    <max>100</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.Pricing.Model.ListPriceListsRequest.CurrencyCode</property>
    <pattern>^[A-Z]{3}$</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.Pricing.Model.ListPriceListsRequest.MaxResults</property>
    <min>1</min>
    <max>100</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.Pricing.Model.ListPriceListsRequest.RegionCode</property>
    <min>1</min>
    <max>255</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.Pricing.Model.ListPriceListsRequest.ServiceCode</property>
    <min>1</min>
    <max>32</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.Pricing.Model.PriceList.CurrencyCode</property>
    <pattern>^[A-Z]{3}$</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.Pricing.Model.PriceList.PriceListArn</property>
    <min>18</min>
    <max>2048</max>
    <pattern>^arn:.+:pricing::.*:price-list/.{1,255}/.{1,32}/[A-Z]{3}/[0-9]{14}/[^/]*$</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.Pricing.Model.PriceList.RegionCode</property>
    <min>1</min>
    <max>255</max>
  </property-value-rule>
</property-value-rules>