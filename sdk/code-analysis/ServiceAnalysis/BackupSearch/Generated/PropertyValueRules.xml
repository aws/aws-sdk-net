    <min>1</min>
    <max>1000</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.BackupSearch.Model.ListSearchJobsRequest.MaxResults</property>
    <min>1</min>
    <max>1000</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.BackupSearch.Model.ListSearchResultExportJobsRequest.MaxResults</property>
    <min>1</min>
    <max>1000</max>
  </property-value-rule>
  <property-value-rule>
