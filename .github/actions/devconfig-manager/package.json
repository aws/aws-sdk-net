{
  "name": "devconfig-manager",
  "version": "1.0.0",
  "description": "DevConfig manager for AWS SDK .NET pull requests",
  "main": "index.js",
  "scripts": {
    "build": "ncc build index.js -o dist --source-map --license licenses.txt",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint *.js lib/*.js",
    "format": "prettier --write *.js lib/*.js"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^5.1.1",
    "@actions/exec": "^1.1.1",
    "simple-git": "^3.19.1"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.38.1",
    "jest": "^29.7.0",
    "eslint": "^8.57.0",
    "prettier": "^3.0.3"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "lib/**/*.js",
      "index.js",
      "!dist/**"
    ]
  },
  "keywords": [
    "github-actions",
    "aws-sdk",
    "devconfig"
  ],
  "author": "AWS SDK Team",
  "license": "Apache-2.0"
}
